#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main(int argc, char *argv[]){
  char *s = (char *)malloc(100);
  char *t = (char *)malloc(100);
  char *names[1000];
  int graph[1000][1000];
  int a;
  int i = 0;
  int j = 0;
  for (i = 0;i < 1000; i++){
    names[i][100] = "0";
  }
  i = 0;
  for (i = 0;i < 1000; i++){
    for (j = 0;j < 1000; j++)
      names[i][j] = 0;
  }
  i = 0;
  int k;
  scanf("%99s", s);
  strcpy(names[0], s);
  scanf("%99s", s);
  strcpy(names[1], s);
  while (scanf("%99s", s) != EOF){
    while (names[i] != NULL){
      if (names[i] == s){
	j = i;
	break;
      }
      else
	i++;
    }
    if (names[i] == 0){
      strcpy(names[i], s);
      j = i;
    }
    i = 0;
    scanf("%99s", t);
    while (names[i] != 0){
      if (names[i] == t){
	k = i;
	break;
      }
      else
	i++;
    }
    if (names[i] == 0){
      strcpy(names[i], t);
      k = i;
    }
    i = 0;
    scanf("%d", &a);
    if (graph[j][k] == 0){
      graph[j][k] = a;
      graph[k][j] = a;
    }
    else if (graph[j][k] > a){
      graph[j][k] = a;
      graph[k][j] = a;
    }
  }

  int b;
  int c;
  b = 100000;
  c = 0;
  for (i = 0; i < 1000; i++){
    if ((graph[0][i] < b) && (graph[0][i] != 0)){
      b = graph[0][i];
      c = i;
    }
  }
  printf("%99s\n", names[c]);
  free(s);
  free(t);
  return 0;
}
    
      
